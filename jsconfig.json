{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@root/*": ["*"],
      "@electron/*": ["electron/*"],
      "@renderer/*": ["src/*"]
    }
  },
  "exclude": ["node_modules", "dist", "dist-electron", "dist-release"],
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.vue", "electron"]
}
