    "--serial=${row.id} --window-title=${\r\n          row.$remark ? `${row.$remark}-` : ''\r\n        }${row.$name}-${\r\n          row.id\r\n        }-ðŸŽ¥å½•åˆ¶ä¸­... --record=${savePath} ${this.scrcpyArgs(row.id)}",
    "--serial=${row.id} --window-title=${\r\n            row.$remark ? `${row.$remark}-` : ''\r\n          }${row.$name}-${row.id} ${this.scrcpyArgs(row.id)}"
  ]
}
